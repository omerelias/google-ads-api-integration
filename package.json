    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "gaxios": "^5.0.0",
    "google-auth-library": "^7.0.0",
    "node-cron": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.0.0",
    "@types/node-cron": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.0"
  }
